<script>
    var navfix = document.querySelector(".header_mid");

    window.addEventListener("scroll", () => {
        if (window.scrollY > 100) {

            navfix.classList.add("active_nav")
        } else {
            navfix.classList.remove("active_nav")
        }
    })
</script>
